{
  "name": "ohmyadmin",
  "version": "0.0.0",
  "engines": {
    "node": ">=24.12.0"
  },
  "scripts": {
    "build": "ng run component-library:build",
    "build:storybook": "ng run component-library:build-storybook",
    "lint": "ng lint --cache",
    "lint:styles": "stylelint projects/component-library/**/*.scss",
    "ng": "ng",
    "start": "ng serve",
    "start:cypress": "cypress open",
    "storybook": "ng run component-library:storybook",
    "test": "VITEST_HEADLESS=false ng run component-library:test",
    "test:ci": "ng run component-library:test --no-watch",
    "test:cypress": "cypress run --component",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.7",
    "@angular/compiler": "^21.0.7",
    "@angular/core": "^21.0.7",
    "@angular/forms": "^21.0.7",
    "@angular/platform-browser": "^21.0.7",
    "@angular/router": "^21.0.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^2.2.1",
    "@analogjs/vite-plugin-angular": "^2.2.0",
    "@analogjs/vitest-angular": "^2.2.1",
    "@angular-devkit/architect": "^0.2100.5",
    "@angular-devkit/build-angular": "^21.0.5",
    "@angular-devkit/core": "^21.0.5",
    "@angular-eslint/builder": "^21.1.0",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.7",
    "@angular/platform-browser-dynamic": "^21.0.7",
    "@compodoc/compodoc": "^1.1.32",
    "@cypress/angular": "^4.1.0",
    "@eslint/js": "^9.0.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/angular": "^10.1.10",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "angular-eslint": "^21.1.0",
    "cypress": "^15.8.1",
    "eslint": "^9.28.0",
    "eslint-plugin-cypress": "^5.2.0",
    "jsdom": "^27.4.0",
    "ng-packagr": "^21.0.0",
    "playwright": "^1.57.0",
    "storybook": "^10.1.10",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.33.1",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.8"
  }
}
