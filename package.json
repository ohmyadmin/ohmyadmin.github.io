{
  "name": "ohmyadmin",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "cypress:open": "cypress open",
    "cypress:start": "cypress open",
    "cypress:test": "cypress run --component",
    "lint": "ng run component-library:lint",
    "ng": "ng",
    "start": "ng serve",
    "storybook": "ng run component-library:storybook",
    "test:cypress:component": "cypress run --component",
    "unit-test": "ng run component-library:test",
    "unit-test:ci": "ng run component-library:test --no-watch",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular-devkit/core": "^21.0.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@compodoc/compodoc": "^1.1.32",
    "@cypress/angular": "^4.1.0",
    "@storybook/angular": "^10.1.10",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@angular-devkit/architect": "^0.2100.0",
    "@vitest/coverage-v8": "^4.0.16",
    "cypress": "^15.8.1",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.2",
    "storybook": "^10.1.10",
    "vitest": "^4.0.8"
  }
}
